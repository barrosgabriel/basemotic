{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "arcanedev/no-captcha": "5.*",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "convertapi/convertapi-php": "^1.1",
        "copam/phpjasper": "^1.4",
        "ilovepdf/ilovepdf-php": "^1.1",
        "laravel/framework": "5.5.4",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "~5.0",
        "lubusin/laravel-decomposer": "^1.2",
        "maatwebsite/excel": "^3.0",
        "orangehill/iseed": "dev-master",
        "owen-it/laravel-auditing": "^7.0",
        "rap2hpoutre/laravel-log-viewer": "^0.19.1",
        "spatie/laravel-backup": "^5.9"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
